<%for (var i=0; i<test.length; i++) {%>


<tr>
    <td> <%= i+1 %> </td>
    <td><%= test[i].name %></td>
    <td><%= test[i].age %></td>
    <td><%= test[i].favouriteFoods %></td>
    <td>
        <a href="/update-user?id=<%= test[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>

        <a class="btn border-shadow delete" data-id= <%= test[i]._id %> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>


    </td>
</tr>
<% } %>